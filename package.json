{
  "name": "react-cli",
  "version": "0.0.0",
  "description": "react project template",
  "private": true,
  "scripts": {
    "test": "jest",
    "start": "webpack",
    "build": "cross-env NODE_ENV=production && webpack",
    "lint:style": "stylelint ./src/**/*.less --fix",
    "lint": "eslint . --ext=.js,.jsx,.ts,.tsx --fix"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:open-data-plan/desktop-template.git"
  },
  "author": "owenwong <owen.wong.cn@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@ant-design/icons": "^4.2.2",
    "antd": "^4.7.3",
    "classnames": "^2.2.6",
    "core-js": "^3.6.5",
    "path-to-regexp": "^6.2.0",
    "rc-animate": "^3.1.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "webpack": "^4.44.2",
    "webpack-cli": "^4.1.0",
    "webpackbar": "^4.0.0",
    "react-dev-utils": "^11.0.0",
    "thread-loader": "^3.0.1",
    "babel-loader": "^8.1.0",
    "style-loader": "^2.0.0",
    "css-loader": "^5.0.0",
    "less-loader": "^7.0.2",
    "postcss-loader": "^4.0.4",
    "postcss-flexbugs-fixes": "^4.2.1",
    "postcss-preset-env": "^6.7.0",
    "postcss-normalize": "^9.0.0",
    "mini-css-extract-plugin": "^1.2.1",
    "terser-webpack-plugin": "^5.0.3",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "workbox-webpack-plugin": "^5.1.4",
    "eslint-webpack-plugin": "^2.1.0",
    "copy-webpack-plugin": "^6.2.1",
    "webpack-manifest-plugin": "^2.2.0",
    "cross-env": "^7.0.2",
    "@otools/react-compile-tools": "^0.0.1",
    "@types/classnames": "^2.2.10",
    "@types/react": "^16.9.54",
    "@types/react-dom": "^16.9.9",
    "@types/react-router": "^5.1.8",
    "@types/react-router-dom": "^5.1.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "*.{css,less}": [
      "stylelint --fix"
    ]
  },
  "prettier": "@otools/prettier-config-config",
  "eslintConfig": {
    "extends": "@otools/eslint-typescript-config"
  },
  "jest": {
    "preset": "@otools/jest-preset-config"
  },
  "browserslist": [
    "last 2 version",
    "IE >= 11"
  ],
  "stylelint": {
    "extends": [
      "@otools/stylelint-config-config"
    ]
  },
  "babel": {
    "presets": [
      "@otools/babel-preset-config"
    ]
  }
}
